{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@actions/*": ["src/actions/*"],
      "@components/*": ["src/components/*"],
      "@constants/*": ["src/constants/*"],
      "@lib/*": ["src/lib/*"],
      "@reducers/*": ["src/reducers/*"]
    }
  }
}
